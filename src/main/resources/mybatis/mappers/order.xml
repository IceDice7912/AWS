<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.order">
	<!-- values 수정해야됨 -->
	<insert id="insert" parameterType="orderVO">
		<![CDATA[
			insert into orders(orderdate,ORDERNO,TOTAL_ORDERPRICE,ORDERQUANTITY,
			ISBN,ID) 
			values(SYSDATE,#{ORDERNO},#{TOTAL_ORDERPRICE},#{ORDERQUANTITY},
			#{ISBN},#{ID})
			
		]]>
	</insert>
	<select id="order_group_no" resultType="long">
        <![CDATA[
            select order_group_no_seq.nextval from dual
        ]]>
	</select>

</mapper>







